<?xml version="1.0" encoding="utf-8"?>
<TLRConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<PackName>One-Size Template</PackName>
	<OneSize>true</OneSize>
	<Assets>
    <!--Be sure to replace "Placeholder" with the prop prefab name for each asset
    use the Pack Creator Helper window to easily copy prop prefab names-->
		<Asset>
			<Prefab>Placeholder</Prefab>
			<Type>Main</Type>
		</Asset>
		<Asset>
			<Prefab>Placeholder</Prefab>
			<Type>Mirror</Type>
		</Asset>
		<Asset>
			<Prefab>Placeholder</Prefab>
			<Type>Ped Signal</Type>
		</Asset>
		<Asset>
			<Prefab>Placeholder</Prefab>
			<Type>Signal Pole</Type>
		</Asset>
	</Assets>
  <!--Optional-->
  <Transform>
    <Position>
      <x>0</x>
      <y>0</y>
      <z>0</z>
    </Position>
    <Angle>0</Angle>
    <Scale>100</Scale>
  </Transform>
</TLRConfig>